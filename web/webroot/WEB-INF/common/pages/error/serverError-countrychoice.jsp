body, html {
  height: 100%;
  margin: 0;
  font-family: "BrandonTextRegular", Helvetica, Arial, sans-serif;
  line-height: 1.5;
  color: #333;
  overflow-x: hidden;
}

.bg {
  background-image: url("https://i1.adis.ws/i/Brakes/error-500-501-lr");
  height: 100%; 
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.top-left {
  position: absolute;
  top: 30%;
  left: 90px;
  right:30px;

}

.logo {
  position: absolute;
  top: 15%;
  left: 47px;
  max-width: 500px;

}
.top-left h1 {
  max-width: 300px;
  font-family: "BrandonTextRegular", Helvetica, Arial, sans-serif;
  font-size: 28px;
  line-height: 35px;
}

h1:after, h2.amp__heading-text:after {
    content: "";
    width: 80px;
    height: 5px;
    display: block;
    margin-top: 7px;
}
.underline:after {
    background: #820085;
}
.top-left p {
  max-width: 500px;
  font-family: "BrandonTextRegular", Helvetica, Arial, sans-serif;
  font-size:20px;
}


.btn-primary {
    border: 1px solid;
    min-width: 50px;
    width: 100%;
    -webkit-transition: background-color 0.3s;
    transition: background-color 0.3s;
    color: #ffffff;
    background-color: #820085;
    border-color: #49155c;
    text-decoration:none;
}

.btn, .btn:active, .btn.active {
    -webkit-box-shadow: none;
    box-shadow: none;
}
.btn {
    width: auto;
    font-family: "BrandonTextBold", Helvetica, Arial, sans-serif;
    font-weight: normal;
    font-style: normal;
    padding: 12px 15px;
    font-size: 18px;
    line-height: 1.375;
    border-radius: 3px;
}


@media only screen and (max-width: 500px) {
    .top-left {
 	  position: absolute;
	  top: 10%;
	  left: 30px;
	  right:30px;
	}
    .logo {
  	 position: absolute;
 	 top: 10px;
	 left: 5px;
  	 max-width: 300px;
	}
}

</style>
