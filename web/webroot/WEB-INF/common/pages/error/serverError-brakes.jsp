.btn {
    width: auto;
    font-family: "BrandonTextBold", Helvetica, Arial, sans-serif;
    font-weight: normal;
    font-style: normal;
    padding: 12px 15px;
    font-size: 18px;
    line-height: 1.375;
    border-radius: 3px;
}


@media only screen and (max-width: 500px) {
    .top-left {
      position: absolute;
      top: 10%;
      left: 30px;
      right:30px;
    }
    .logo {
     position: absolute;
     top: 10px;
     left: 22px;
